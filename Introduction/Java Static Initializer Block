static int B;
static int H;
static boolean flag;

static{
    Scanner scan = new Scanner(System.in);
    B = scan.nextInt();
    H = scan.nextInt();
    flag = false;
    if((B>0 && B<=100) && (H>0 && H<=100)){
        flag=true;
    }
    else{
        System.out.println("java.lang.Exception: Breadth and height must be positive");
    }
}
